<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<ProjectGuid>{820694DF-C225-46B6-BA38-C5FEFBD79AEB}</ProjectGuid>
		<TargetFramework>net48</TargetFramework>
		<WebGreaseLibPath>..\packages\WebGrease.1.5.2\lib</WebGreaseLibPath>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<GlobalOutputPath>..\bin\$(Configuration)\</GlobalOutputPath>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
		<OutputPath Condition="$(TargetFramework) == 'net48'">$(GlobalOutputPath)</OutputPath>
		<OutputPath Condition="$(TargetFramework) != 'net48'">$(GlobalOutputPath)$(TargetFramework)\</OutputPath>
		<SignAssembly>true</SignAssembly>
		<AssemblyOriginatorKeyFile>..\AjaxKey.snk</AssemblyOriginatorKeyFile>
		<RestoreSources>$(RestoreSources);../../../nupkg;https://api.nuget.org/v3/index.json</RestoreSources>

		<Title>AjaxControlToolkit.StaticResources</Title>
		<Product>AjaxControlToolkit.StaticResources</Product>
		<Description>AjaxControlToolkit for WebFormsForCore</Description>
	</PropertyGroup>
	<ItemGroup Condition="$(TargetFramework) == 'net48'">
		<PackageReference Include="Microsoft.AspNet.Web.Optimization" Version="1.1.3" />
		<PackageReference Include="Microsoft.Web.Infrastructure" Version="2.0.0" />
		<PackageReference Include="WebGrease" Version="1.6.0" />
	</ItemGroup>
	<ItemGroup Condition="$(TargetFramework) != 'net48'">
		<!--<PackageReference Include="WebFormsForCore.Web.Optimization" Version="1.1.2-beta" />
        <PackageReference Include="WebFormsForCore.Web.Infrastructure" Version="1.1.2-beta" />
        <PackageReference Include="WebFormsForCore.WebGrease" Version="1.1.2-beta" />-->
		<ProjectReference Include="..\..\WebFormsForCore.Web.Optimization\WebFormsForCore.Web.Optimization.csproj" />
		<ProjectReference Include="..\..\WebFormsForCore.Web.Infrastructure\WebFormsForCore.Web.Infrastructure.csproj" />
		<ProjectReference Include="..\..\WebFormsForCore.WebGrease\WebFormsForCore.WebGrease.csproj" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Antlr" Version="3.5.0.2" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
	</ItemGroup>
	<ItemGroup>
		<Compile Include="..\SharedAssemblyInfo.cs" Link="Properties\SharedAssemblyInfo.cs" />
	</ItemGroup>
	<ItemGroup>
		<Reference Include="System.Web" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\AjaxControlToolkit\AjaxControlToolkit.csproj" />
	</ItemGroup>

	<ItemGroup>
		<None Include="..\..\..\README.md">
